print(1, 2, 3, 4, sep="-")

for i in range(5):
    print(i)


for i in range(5):
    print(i, end="-")

print()

for i in range(5):
    print(f"{i:15}", end="")

print()
for i in range(5):
    print("i", i)

    for j in range(5):
        print("j", j)
        print("i*j", i*j)


print("   ", end="")
for i in range(10):
    print(f"{i:3}", end="")
print()
for i in range(10):
    print(i, end="  ")
    for j in range(10):
        print(f"{i*j:3}", end="")
    print()